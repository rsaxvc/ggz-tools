        <groundspeak:cache archived="False" available="True" id="1088598">
            <groundspeak:name>X</groundspeak:name>
            <groundspeak:placed_by>LUCHTFIETS</groundspeak:placed_by>
            <groundspeak:owner id="2051">LUCHTFIETS</groundspeak:owner>
