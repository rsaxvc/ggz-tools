        <desc>Op de weide by Dutchie-WbD, Traditional Cache(1.5/1.5/2.9)</desc>
        <url>http://opencachetest.garmin.com/#geocache/OX32NDT</url>
        <urlname>Op de weide</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2850754">
            <groundspeak:name>Op de weide</groundspeak:name>
            <groundspeak:placed_by>Dutchie-WbD</groundspeak:placed_by>
