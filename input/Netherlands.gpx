        <url>http://opencachetest.garmin.com/#geocache/OXRWVQ</url>
        <urlname>Het Paradijsje/Little Paradise (Chirp)</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="741760">
            <groundspeak:name>Het Paradijsje/Little Paradise (Chirp)</groundspeak:name>
            <groundspeak:placed_by>Team L2</groundspeak:placed_by>
