        <urlname>Rondje Overloon</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="756503">
            <groundspeak:name>Rondje Overloon</groundspeak:name>
            <groundspeak:placed_by>ROBENADA</groundspeak:placed_by>
            <groundspeak:owner id="5630">ROBENADA</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
