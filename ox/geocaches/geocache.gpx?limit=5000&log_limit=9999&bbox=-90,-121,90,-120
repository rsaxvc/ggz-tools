        <name>OX27G9Q</name>
        <desc>Columbia's Labyrinth by 4x4Husky, Traditional Cache(2.5/4.2/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX27G9Q</url>
        <urlname>Columbia's Labyrinth</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2071257">
            <groundspeak:name>Columbia's Labyrinth</groundspeak:name>
