            <groundspeak:name>Over The Fence</groundspeak:name>
            <groundspeak:placed_by>pirate_rat</groundspeak:placed_by>
            <groundspeak:owner id="1518">pirate_rat</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
