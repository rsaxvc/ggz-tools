        <groundspeak:cache archived="False" available="True" id="2400908">
            <groundspeak:name>Microburst</groundspeak:name>
            <groundspeak:placed_by>ky.m.guy</groundspeak:placed_by>
            <groundspeak:owner id="2820">ky.m.guy</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>1.5</groundspeak:difficulty>
