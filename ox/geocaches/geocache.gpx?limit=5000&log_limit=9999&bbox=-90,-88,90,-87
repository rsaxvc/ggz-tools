        <desc>TaW#1 - Turtle among Wildcats #1 - Mini Forest by TurtleFan, Traditional Cache(2.1/1.5/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX1Y88Q</url>
        <urlname>TaW#1 - Turtle among Wildcats #1 - Mini Forest</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1795419">
            <groundspeak:name>TaW#1 - Turtle among Wildcats #1 - Mini Forest</groundspeak:name>
            <groundspeak:placed_by>TurtleFan</groundspeak:placed_by>
            <groundspeak:owner id="6967">TurtleFan</groundspeak:owner>
