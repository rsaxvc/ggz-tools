        <groundspeak:cache archived="False" available="True" id="583092">
            <groundspeak:name>This is where I dropped my phone.  :(</groundspeak:name>
            <groundspeak:placed_by>Panther</groundspeak:placed_by>
            <groundspeak:owner id="2864">Panther</groundspeak:owner>
