        <name>OX29Y9G</name>
        <desc>CCCNP: Lion Rock Cache by Rubicon Cacher, Traditional Cache(2.0/3.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX29Y9G</url>
        <urlname>CCCNP: Lion Rock Cache</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2143325">
            <groundspeak:name>CCCNP: Lion Rock Cache</groundspeak:name>
            <groundspeak:placed_by>Rubicon Cacher</groundspeak:placed_by>
            <groundspeak:owner id="15801">Rubicon Cacher</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Large</groundspeak:container>
