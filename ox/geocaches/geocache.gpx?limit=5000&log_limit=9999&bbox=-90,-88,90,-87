        <name>OX19ZGW</name>
        <desc>The Neighborhood by BRETZAKKRUMP, Unknown Cache(3.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX19ZGW</url>
        <urlname>The Neighborhood</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1220993">
            <groundspeak:name>The Neighborhood</groundspeak:name>
            <groundspeak:placed_by>BRETZAKKRUMP</groundspeak:placed_by>
            <groundspeak:owner id="125993">BRETZAKKRUMP</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>3</groundspeak:difficulty>
