        <name>OX2HWC5</name>
        <desc>BB's &quot;The Curse (13) of the Goatman&quot; Series by BAYTOWNBERT, Traditional Cache(2.0/2.8/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2HWC5</url>
        <urlname>BB's &quot;The Curse (13) of the Goatman&quot; Series</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2379813">
            <groundspeak:name>BB's &quot;The Curse (13) of the Goatman&quot; Series</groundspeak:name>
            <groundspeak:placed_by>BAYTOWNBERT</groundspeak:placed_by>
            <groundspeak:owner id="1281">BAYTOWNBERT</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
