        <groundspeak:cache archived="False" available="True" id="28478340">
            <groundspeak:name>The Pendulum</groundspeak:name>
            <groundspeak:placed_by>kacer.kacer</groundspeak:placed_by>
            <groundspeak:owner id="3874">kacer.kacer</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
