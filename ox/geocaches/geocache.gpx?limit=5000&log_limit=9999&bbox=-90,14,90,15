            <groundspeak:name>Werfensteinblick</groundspeak:name>
            <groundspeak:placed_by>Strudner</groundspeak:placed_by>
            <groundspeak:owner id="4778">Strudner</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
