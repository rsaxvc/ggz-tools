        <name>OX260KW</name>
        <desc>Hanna Bieber-Böhm by Giochetto, Traditional Cache(1.6/2.9/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OX260KW</url>
        <urlname>Hanna Bieber-Böhm</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2026404">
            <groundspeak:name>Hanna Bieber-Böhm</groundspeak:name>
            <groundspeak:placed_by>Giochetto</groundspeak:placed_by>
            <groundspeak:owner id="6981">Giochetto</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
