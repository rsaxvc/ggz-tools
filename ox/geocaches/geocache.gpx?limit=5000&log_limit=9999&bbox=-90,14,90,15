        <desc>Kalvarienberg-Kreuzweg by wandersmann_OOE, Multi-cache(2.6/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1B0PY</url>
        <urlname>Kalvarienberg-Kreuzweg</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1251933">
            <groundspeak:name>Kalvarienberg-Kreuzweg</groundspeak:name>
            <groundspeak:placed_by>wandersmann_OOE</groundspeak:placed_by>
            <groundspeak:owner id="5749">wandersmann_OOE</groundspeak:owner>
