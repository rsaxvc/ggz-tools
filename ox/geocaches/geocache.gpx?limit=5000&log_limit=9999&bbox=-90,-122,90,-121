            <groundspeak:name>Carolyn Norris Park and Dash</groundspeak:name>
            <groundspeak:placed_by>jetta395</groundspeak:placed_by>
            <groundspeak:owner id="18640">jetta395</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
