        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2739027">
            <groundspeak:name>Lunch on the Rocks</groundspeak:name>
            <groundspeak:placed_by>galileo_the_cat</groundspeak:placed_by>
            <groundspeak:owner id="4612">galileo_the_cat</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
