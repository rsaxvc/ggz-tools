        <name>OX23EZJ</name>
        <desc>O.L. Peacock, Sr. Lake Trail #9 by KrumBen, Traditional Cache(1.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX23EZJ</url>
        <urlname>O.L. Peacock, Sr. Lake Trail #9</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1950817">
            <groundspeak:name>O.L. Peacock, Sr. Lake Trail #9</groundspeak:name>
            <groundspeak:placed_by>KrumBen</groundspeak:placed_by>
