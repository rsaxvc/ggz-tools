        <name>OX2A7WG</name>
        <desc>I am the SWORD MASTER!!! by Pasha88, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2A7WG</url>
        <urlname>I am the SWORD MASTER!!!</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2152532">
            <groundspeak:name>I am the SWORD MASTER!!!</groundspeak:name>
