        <name>OX2TTHJ</name>
        <desc>GCC series - Smellie's Cemetery by Ausiesrock, Traditional Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2TTHJ</url>
        <urlname>GCC series - Smellie's Cemetery</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2616387">
            <groundspeak:name>GCC series - Smellie's Cemetery</groundspeak:name>
            <groundspeak:placed_by>Ausiesrock</groundspeak:placed_by>
