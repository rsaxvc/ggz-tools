        <name>OX2186H</name>
        <desc>Bell Creek Combo Cache by Ausiesrock, Multi-cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2186H</url>
        <urlname>Bell Creek Combo Cache</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1884724">
            <groundspeak:name>Bell Creek Combo Cache</groundspeak:name>
            <groundspeak:placed_by>Ausiesrock</groundspeak:placed_by>
            <groundspeak:owner id="7883">Ausiesrock</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
