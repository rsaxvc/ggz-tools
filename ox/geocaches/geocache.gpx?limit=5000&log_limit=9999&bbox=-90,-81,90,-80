        <groundspeak:cache archived="False" available="True" id="2540679">
            <groundspeak:name>You know you're a geocacher when...</groundspeak:name>
            <groundspeak:placed_by>Pasha88</groundspeak:placed_by>
            <groundspeak:owner id="49764">Pasha88</groundspeak:owner>
