            <groundspeak:name>M&amp;M-148 Oscar the Grouch</groundspeak:name>
            <groundspeak:placed_by>M&amp;M_Melted</groundspeak:placed_by>
            <groundspeak:owner id="941">M&amp;M_Melted</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
