        <desc>Fallin' For Virginia XVII - Parks Mill by KONNAROCK KID, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXY8RX</url>
        <urlname>Fallin' For Virginia XVII - Parks Mill</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="872399">
            <groundspeak:name>Fallin' For Virginia XVII - Parks Mill</groundspeak:name>
            <groundspeak:placed_by>KONNAROCK KID</groundspeak:placed_by>
            <groundspeak:owner id="5264">KONNAROCK KID</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
