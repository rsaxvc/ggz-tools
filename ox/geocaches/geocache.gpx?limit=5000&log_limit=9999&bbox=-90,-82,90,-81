        <groundspeak:cache archived="False" available="True" id="746352">
            <groundspeak:name>Stetson's Cache</groundspeak:name>
            <groundspeak:placed_by>CLANRIFFSTER</groundspeak:placed_by>
            <groundspeak:owner id="4976">CLANRIFFSTER</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
