        <url>http://www.opencaching.com/#geocache/OX82FD</url>
        <urlname>The Long and Winding Trail</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="240728">
            <groundspeak:name>The Long and Winding Trail</groundspeak:name>
            <groundspeak:placed_by>CLANRIFFSTER</groundspeak:placed_by>
