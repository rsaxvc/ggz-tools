        <name>OXY6ZF</name>
        <desc>Geocachers Bill of Rights by CLANRIFFSTER, Unknown Cache(3.0/3.0/3.5)</desc>
        <url>http://www.opencaching.com/#geocache/OXY6ZF</url>
        <urlname>Geocachers Bill of Rights</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="870650">
            <groundspeak:name>Geocachers Bill of Rights</groundspeak:name>
            <groundspeak:placed_by>CLANRIFFSTER</groundspeak:placed_by>
            <groundspeak:owner id="4976">CLANRIFFSTER</groundspeak:owner>
