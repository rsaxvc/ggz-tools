        <name>OX103JF</name>
        <desc>Up and Over by Geo-Sarge, Multi-cache(1.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX103JF</url>
        <urlname>Up and Over</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="926977">
            <groundspeak:name>Up and Over</groundspeak:name>
            <groundspeak:placed_by>Geo-Sarge</groundspeak:placed_by>
            <groundspeak:owner id="7021">Geo-Sarge</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
