        <name>OX1AWV3</name>
        <desc>Steeples and Stained Glass - Zion Lutheran by TeamDotOne, Traditional Cache(1.5/1.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1AWV3</url>
        <urlname>Steeples and Stained Glass - Zion Lutheran</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1248187">
            <groundspeak:name>Steeples and Stained Glass - Zion Lutheran</groundspeak:name>
            <groundspeak:placed_by>TeamDotOne</groundspeak:placed_by>
            <groundspeak:owner id="4639">TeamDotOne</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
