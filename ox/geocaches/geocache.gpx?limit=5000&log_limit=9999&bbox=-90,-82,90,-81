        <name>OXZTXW0</name>
        <desc>Walk In The Park by RROSS1973, Traditional Cache(2.5/2.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTXW0</url>
        <urlname>Walk In The Park</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28478150">
            <groundspeak:name>Walk In The Park</groundspeak:name>
