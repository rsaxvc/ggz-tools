        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="583677">
            <groundspeak:name>Bogging Down In The Orchard</groundspeak:name>
            <groundspeak:placed_by>KONNAROCK KID</groundspeak:placed_by>
            <groundspeak:owner id="5264">KONNAROCK KID</groundspeak:owner>
