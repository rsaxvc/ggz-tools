        <name>OX1WCE8</name>
        <desc>Difficulty 3 (Ontario Mini-Fizzy 9 Challenge) by northernpenguin, Unknown Cache(3.0/4.8/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX1WCE8</url>
        <urlname>Difficulty 3 (Ontario Mini-Fizzy 9 Challenge)</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1739852">
            <groundspeak:name>Difficulty 3 (Ontario Mini-Fizzy 9 Challenge)</groundspeak:name>
            <groundspeak:placed_by>northernpenguin</groundspeak:placed_by>
            <groundspeak:owner id="826">northernpenguin</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
