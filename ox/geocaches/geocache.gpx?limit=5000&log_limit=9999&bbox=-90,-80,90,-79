        <name>OX1701A</name>
        <desc>GC1701A: Virtual Beginnings and Memories in Hilton by res2100, Multi-cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1701A</url>
        <urlname>GC1701A: Virtual Beginnings and Memories in Hilton</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1132099">
            <groundspeak:name>GC1701A: Virtual Beginnings and Memories in Hilton</groundspeak:name>
            <groundspeak:placed_by>res2100</groundspeak:placed_by>
            <groundspeak:owner id="4660">res2100</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
