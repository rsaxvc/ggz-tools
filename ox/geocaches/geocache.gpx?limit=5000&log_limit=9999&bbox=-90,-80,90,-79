        <name>OX2Z88Y</name>
        <desc>Lonely Credit by res2100, Multi-cache(1.0/5.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2Z88Y</url>
        <urlname>Lonely Credit</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2748737">
            <groundspeak:name>Lonely Credit</groundspeak:name>
            <groundspeak:placed_by>res2100</groundspeak:placed_by>
            <groundspeak:owner id="4660">res2100</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
