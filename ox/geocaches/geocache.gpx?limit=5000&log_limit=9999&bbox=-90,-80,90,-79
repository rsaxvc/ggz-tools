        <groundspeak:cache archived="False" available="True" id="1772558">
            <groundspeak:name>Franklin's First Cache</groundspeak:name>
            <groundspeak:placed_by>sequens_vitae_mortem</groundspeak:placed_by>
            <groundspeak:owner id="7507">sequens_vitae_mortem</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>1.5</groundspeak:difficulty>
