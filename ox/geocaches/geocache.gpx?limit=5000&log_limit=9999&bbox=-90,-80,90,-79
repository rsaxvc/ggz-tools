            <groundspeak:name>B.Y. the Cliffs</groundspeak:name>
            <groundspeak:placed_by>Whizard</groundspeak:placed_by>
            <groundspeak:owner id="12216">Whizard</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
