        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="923446">
            <groundspeak:name>North America - United States of America</groundspeak:name>
            <groundspeak:placed_by>res2100</groundspeak:placed_by>
            <groundspeak:owner id="4660">res2100</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Large</groundspeak:container>
            <groundspeak:difficulty>1</groundspeak:difficulty>
